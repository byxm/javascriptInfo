ktop/demo/javascriptInfo/react-client/src/demo/useContext/context.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/useReducer/index.js",["115","116","117","118"],"import React, { useReducer } from \"react\";\nimport { count1, es1Count } from './esModule1'\nimport { count2, es2Count } from './esModule2'\n\nfunction myReducer(state, action) {\n  switch (action.type) {\n    case \"add\":\n      return {\n        ...state,\n        count: state.count + 1,\n      };\n    case \"sub\":\n      return {\n        ...state,\n        count: state.count - 1,\n      };\n    default:\n      return {\n        ...state,\n      };\n  }\n}\n\nfunction ReducerDemo() {\n  const [state, dispatch] = useReducer(myReducer, { count: 0 });\n  // es1Count(10)\n  // console.log('outEs1', count1);\n  // es2Count(10)\n  // console.log('outEs2', count2);\n\n  \n\n  return (\n    <div>\n      <button\n        onClick={() => {\n          dispatch({ type: \"add\" });\n        }}\n      >\n        状态按钮\n      </button>\n      <p>{state.count}</p>\n    </div>\n  );\n}\n\nexport default ReducerDemo;\n","/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/useEffect/index.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/useEffect/useUserInfo.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/useEffect/memorize.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/project/index.js",["119","120","121","122","123"],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/useReducer/esModule1.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/useReducer/esModule2.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/react-dnd/index.js",["124"],"import React from \"react\";\nimport Content from \"./content\";\nimport { DndProvider } from \"react-dnd\";\nimport { HTML5Backend } from \"react-dnd-html5-backend\";\nimport { observe } from \"./game\";\n\nconst ReactDnd = ({ knightPosition }) => {\n  return (\n    <DndProvider backend={HTML5Backend}>\n      <Content knightPosition={knightPosition} />;\n    </DndProvider>\n  );\n};\n\nexport default ReactDnd;\n","/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/react-dnd/knight.js",["125"],"import React, { useRef } from \"react\";\nimport { ItemTypes, knightImage } from \"../common\";\nimport { useDrag, DragPreviewImage } from \"react-dnd\";\n\nconst Knight = () => {\n  const ref = useRef(null);\n  const [{ isDragging }, drag, preview] = useDrag(() => ({\n    type: ItemTypes.KNIGHT,\n    collect: (monitor) => ({ isDragging: !!monitor.isDragging() }),\n  }));\n\n  return (\n    <>\n      <DragPreviewImage connect={preview} src={knightImage} />\n      <span\n        ref={drag}\n        style={{\n          opacity: isDragging ? 0.5 : 1,\n          fontSize: 70,\n          fontWeight: \"bold\",\n          cursor: \"move\",\n        }}\n      >\n        ♘\n      </span>\n    </>\n  );\n};\n\nexport default Knight;\n","/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/react-dnd/square.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/react-dnd/game.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/react-dnd/content.js",["126","127","128"],"import React from \"react\";\nimport Knight from \"./knight\";\nimport Square from \"./square\";\nimport BoardSquare from \"./boardSquare\";\nimport { moveKnight, canMoveKnight, observe } from \"./game\";\n\n/* ... */\n\n// function renderSquare(i, knightPosition) {\n//   /* ... */\n//   return <div onClick={() => handleSquareClick(x, y)}>{/* ... */}</div>;\n// }\n\nfunction handleSquareClick(toX, toY) {\n  console.log(\"toX, toY\", toX, toY, canMoveKnight(toX, toY));\n  if (canMoveKnight(toX, toY)) {\n    moveKnight(toX, toY);\n  }\n}\n\nfunction renderSquare(i, knightPosition) {\n  const x = i % 8;\n  const y = Math.floor(i / 8);\n  // const isKnightHere = x === knightX && y === knightY;\n  // const black = (x + y) % 2 === 1;\n  // const piece = isKnightHere ? <Knight /> : null;\n\n  return (\n    <div\n      key={i}\n      style={{ width: \"12.5%\", height: \"12.5%\" }}\n    >\n      <BoardSquare x={x} y={y}>\n        {renderPiece(x, y, knightPosition)}\n      </BoardSquare>\n    </div>\n  );\n}\n\nfunction renderPiece(x, y, [knightX, knightY]) {\n  if (x === knightX && y === knightY) {\n    return <Knight />\n  }\n}\n\nconst ReactDnd = ({ knightPosition }) => {\n  const squares = [];\n  for (let i = 0; i < 64; i++) {\n    squares.push(renderSquare(i, knightPosition));\n  }\n\n  return (\n    <div\n      style={{\n        width: \"100%\",\n        height: \"100%\",\n        display: \"flex\",\n        flexWrap: \"wrap\",\n      }}\n    >\n      {squares}\n    </div>\n  );\n};\n\nexport default ReactDnd;\n","/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/common/index.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/react-dnd/boardSquare.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/react-dnd/overlay.js",[],"/Users/chenpanyu/Desktop/demo/javascriptInfo/react-client/src/demo/useRef/index.js",["129"],{"ruleId":"130","severity":1,"message":"131","line":5,"column":50,"nodeType":"132","messageId":"133","endLine":5,"endColumn":54},{"ruleId":"130","severity":1,"message":"134","line":11,"column":8,"nodeType":"132","messageId":"133","endLine":11,"endColumn":15},{"ruleId":"130","severity":1,"message":"135","line":12,"column":8,"nodeType":"132","messageId":"133","endLine":12,"endColumn":16},{"ruleId":"130","severity":1,"message":"136","line":13,"column":10,"nodeType":"132","messageId":"133","endLine":13,"endColumn":17},{"ruleId":"130","severity":1,"message":"137","line":2,"column":8,"nodeType":"132","messageId":"133","endLine":2,"endColumn":12},{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","replacedBy":"141"},{"ruleId":"130","severity":1,"m